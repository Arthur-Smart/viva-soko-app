{
  "expo": {
    "name": "viva-soko",
    "slug": "viva-soko",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/viva-launch-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Viva--Soko-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.soko.arthur"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.vivo.arthur"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
